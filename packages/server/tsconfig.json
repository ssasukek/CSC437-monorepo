{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "./src",
    "rootDir": "src", 
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true
  },
  // "include": ["src/**/*.ts", "models/cardData.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
  "lib": ["esnext"]
}