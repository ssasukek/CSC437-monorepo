{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "baseUrl": "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "bundler",
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "models/cardData.ts"],
    "exclude": ["node_modules"],
    "lib": ["esnext"]
  }